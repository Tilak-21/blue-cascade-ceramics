# Application Configuration
REACT_APP_NAME=TileCraft Premium
REACT_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=development

# API Configuration
REACT_APP_API_BASE_URL=https://api.tilecraftpremium.com
REACT_APP_API_TIMEOUT=10000

# Monitoring and Analytics
REACT_APP_SENTRY_DSN=your_sentry_dsn_here
REACT_APP_GOOGLE_ANALYTICS_ID=GA-XXXXXXXXX
REACT_APP_HOTJAR_ID=your_hotjar_id

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_ERROR_REPORTING=true
REACT_APP_ENABLE_PERFORMANCE_MONITORING=true
REACT_APP_ENABLE_DEBUG_MODE=false

# Contact Information
REACT_APP_SUPPORT_EMAIL=support@tilecraftpremium.com
REACT_APP_SALES_EMAIL=sales@tilecraftpremium.com
REACT_APP_PHONE=+1-555-123-4567

# Social Media
REACT_APP_LINKEDIN_URL=https://linkedin.com/company/tilecraft-premium
REACT_APP_INSTAGRAM_URL=https://instagram.com/tilecraftpremium
REACT_APP_FACEBOOK_URL=https://facebook.com/tilecraftpremium

# Security
REACT_APP_ENABLE_CSP=true
REACT_APP_ALLOWED_DOMAINS=tilecraftpremium.com,*.tilecraftpremium.com

# Performance
REACT_APP_IMAGE_CDN_URL=https://cdn.tilecraftpremium.com
REACT_APP_ENABLE_SERVICE_WORKER=true
REACT_APP_CACHE_DURATION=3600000